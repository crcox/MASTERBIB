@Article{Dixon99,
  author = 	 "Dixon, M.",
  title = 	 "Tool and bird exemplar identification in a patient with 
               category-specific visual agnosia.",
  journal =	 "Brain and Cognition",
  year =	 1999,
  volume =	 40,
  number =	 1,
  pages =	 "97-100",
  annote =	 "Tool and bird exemplar identification was investigated in a 
               71-yr-old S with category-specific identification deficits. 
               Instead of line drawings, novel computer generated shapes 
               were paired with either tool or bird exemplar labels, and the 
               S was asked to remember which label went with which shape. By 
               having the same shapes and for tool and bird exemplars, 
               visual similarity among exemplars within the 2 categories was 
               exactly equated. The S's significantly better identification 
               of shapes bearing tool labels was attributed to tools being 
               better separated semantically than birds. Such differences in 
               semantic distance were likely because tool exemplars have 
               different functions for people. ((c) 1999 APA/PsycINFO, all 
               rights reserved):"
}

@Article{DixonBubArguin97,
  author = 	 "Dixon, M. and Bub, Daniel and Arguin, Martin",
  title = 	 "The interaction of object form and object meaning in the 
               identification performance of a patient with category-
               specific visual agnosia",
  journal =	 "Cognitive Neuropsychology",
  year =	 1997,
  volume =	 14,
  number =	 8,
  pages =	 "1085-1130",
  annote =	 "Category-specific visual agnosia following bilateral inferior 
               temporal lobe stroke was investigated in an elderly male 
               patient. Exp 1 verified that computer-generated blobs could 
               not be identified when members of a set varied along a single 
               but not along multiple shape dimensions. Exps 2-6 showed that 
               for both the patient and, to a much lesser degree, healthy 
               Ss, this dimensionality effect was modulated by semantics. By 
               pairing the exact same shapes with semantically close vs 
               disparate sounds or labels, the role of an object's semantics 
               in category-specific agnosia was assessed independently from 
               object form. For single-dimension shape sets, the semantic 
               proximity of the concepts associated with the shapes had no 
               impact on the patient's identification performance. For 
               multidimensional shape sets, the patient's error rates showed 
               a strong positive correlation with semantic proximity. 
               Results are interpreted using an exemplar model of 
               categorization in which a deficit in exemplar node 
               specificity is assumed. It is concluded that biological 
               objects are more likely than nonbiological objects to have 
               the combination of semantic proximity and shared values along 
               multiple shape dimensions that pose recognition problems for 
               patients with such specificity deficits. ((c) 1998 
               APA/PsycINFO, all rights reserved)"
}

@Article{DixonBubArguin98,
  author = 	 "Dixon, M. and Bub, Daniel and Arguin, Martin",
  title = 	 "Semantic and visual determinants of face recognition in a 
               prosopagnosic patient",
  journal =	 "Journal of Cognitive Neuroscience",
  year =	 1998,
  volume =	 10,
  number =	 3,
  pages =	 "362-276",
  annote =	 "Examined whether the face recognition of a 66-yr-old male 
               patient with temporal lobe damage and prosopagnosia is 
               constrained by the fact that faces share multiple visual 
               features and are semantically close. The prosopagnosic 
               patient had difficulties pairing faces to names when the 
               faces shared visual features and the names were semantically 
               related. He made ten-fold fewer errors when the exact same 
               faces were associated with semantically unrelated people. The 
               authors conclude that prosopagnosia and co-occurring category-
               specific recognition problems both stem from difficulties 
               disambiguating the stored representations of objects that 
               share multiple visual features and refer to semantically 
               close identities or concepts. ((c) 1998 APA/PsycINFO, all 
               rights reserved)"
}

@Article{DixonBubChertkow99,
  author = 	 "Dixon, M. and Bub, Daniel and Chertkow, Howard",
  title = 	 "Object identification deficits in dementia of the Alzheimer 
               type: Combined effects of semantic and visual proximity.",
  journal =	 "Journal of the International Neuropsychological Society",
  year =	 1999,
  volume =	 5,
  number =	 4,
  pages =	 "330-345",
  month =	 "May",
  annote =	 "Used computer generated stimuli with empirically specifiable 
               properties in a paradigm that decoupled form and meaning to 
               examine object category-specific identification deficits in 
               dementia of the Alzheimer type (DAT). In Exp 1, visually 
               similar or distinct blobs were paired with semantically close 
               or disparate labels, and Ss (8 65-85 yr old DAT patients and 
               8 elderly controls) attempted to learn these pairings. By 
               having the same blobs stand for semantically close and 
               disparate objects and by looking at shape-label confusion 
               rates for each type of set, form and meaning were 
               independently assessed. Overall, visual similarity of shapes 
               and semantic similarity of labels each exacerbated object 
               confusions. For controls, the effects were small but 
               significant. For DAT Ss, more substantial visual and semantic 
               proximity effects were obtained. In Exp 2, with 7 of 8 DAT 
               patients acting as Ss, even small changes in semantic 
               proximity effected significant changes in DAT task 
               performance. Labeling 3 blobs as `lion,' `tiger,' and 
               `leopard' significantly elevated DAT confusion rates compared 
               to exactly the same blobs labeled as `lion,' `tiger,' and 
               `zebra.' Both visual similarity and semantic proximity 
               contributed to the identification errors of DAT patients. 
               ((c) 1999 APA/PsycINFO, all rights reserved)"
}

