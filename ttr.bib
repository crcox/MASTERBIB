@Article{SpitzerETAL95,
  author = 	 "Spitzer, Manfred and Kwong, Kenneth K. and Kennedy,
		  William and Rosen, Bruce R.",
  title = 	 " Category-specific brain activation in fMRI during picture 
               naming",
  journal =	 "Neuroreport: An International Journal for the Rapid 
               Communication of Research in Neuroscience",
  year =	 1995,
  volume =	 6,
  number =	 16,
  pages =	 "2109-2112",
  month =	 "November",
  annote =	 "Studied the existence of semantic maps, or localized 
               representations of semantic information in the brain, using 
               functional magnetic resonance imaging (MRI) and a picture 
               naming task. Five males performed a covert naming task on 
               items from 4 categories: furniture, fruit, animals, and 
               tools. Color images of 20 items from each category were taken 
               from a visual dictionary. One 6 min data acquisition run 
               consisted of 3 presentations of each series of category 
               specific items in fixed succession. Four such runs were 
               performed by each S. While performing the task, Ss were 
               scanned using echo planar imaging. Results indicate that 
               there are multiple maps coding high-level representations of 
               objects, such that meaningful distinctions govern the 
               physical distribution of cortical semantic storage. It is 
               suggested that the cortical representation of semantic 
               information is partly localized. ((c) 1997 APA/PsycINFO, all 
               rights reserved)"
}

@Article{SpitzerETAL98,
  author = 	 "Spitzer, Manfred and Kischka, Udo and Gueckel,
		  Friedemann amd Bellemann, Matthias E. and Kammer and
		  Thomas and Seyyedi, Soraya and
               Weisbrod, Matthias and Schwartz, Andreas and Brix, Gunnar",
  title = 	 "Functional magnetic resonance imaging of category-specific 
               cortical activation: Evidence for semantic maps",
  journal =	 "Cognitive Brain Research",
  year =	 1998,
  volume =	 6,
  number =	 4,
  pages =	 "309-319",
  month =	 "April",
  annote =	 "Functional magnetic resonance imaging (fMRI) was used to 
               examine the pattern of cortical activity during a picture 
               naming task. 20-38 yr olds Ss (n = 12) had to covertly name 
               either animals or furniture items. Images obtained during 
               naming the two categories were compared using a non-
               parametric test. The study revealed evidence for domain-
               specific lexical regions in left middle, right middle and 
               inferior frontal areas, as well as in superior and middle 
               temporal areas. The results corroborate neuropsychological 
               data and demonstrate directly and non-invasively in human 
               volunteers that semantic representations in frontal and 
               temporal areas are, to some degree, localized and possibly 
               implemented as multiple maps. A completely distributed 
               storage of semantic information is rendered unlikely. ((c) 
               1998 APA/PsycINFO, all rights reserved)"
}

