  Shorttitle               = {Object recognition under semantic impairment},
  Url                      = {http://www.tandfonline.com/doi/abs/10.1080/01690960344000053},
  Urldate                  = {2013-06-04}
}

@Article{rogers_semantic_2006,
  Title                    = {Semantic memory in Alzheimer's disease and the frontotemporal dementias: A longitudinal study of 236 patients},
  Author                   = {Rogers, Timothy T. and Ivanoiu, Adrian and Patterson, Karalyn and Hodges, John R.},
  Journal                  = {Neuropsychology},
