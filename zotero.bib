  File                     = {Full Text PDF:/Users/lcnllab/Library/Application Support/Zotero/Profiles/oh6jsc30.default/zotero/storage/24MMU52Z/Verheyen et al. - 2007 - Determining the dimensionality in spatial represen.pdf:application/pdf;Snapshot:/Users/lcnllab/Library/Application Support/Zotero/Profiles/oh6jsc30.default/zotero/storage/JK4N4ZTW/BF03193012.html:text/html},
  ISSN                     = {1554-{351X}, 1554-3528},
  Keywords                 = {Cognitive Psychology},
  Language                 = {en},
