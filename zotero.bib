@Article{anzellotti_differential_2010,
  Title                    = {Differential Activity for Animals and Manipulable Objects in the Anterior Temporal Lobes},
  Author                   = {Anzellotti, Stefano and Mahon, Bradford Z. and Schwarzbach, Jens and Caramazza, Alfonso},
  Journal                  = {Journal of Cognitive Neuroscience},
