@Article{portas_auditory_2000,
  Title                    = {Auditory processing across the sleep-wake cycle: simultaneous {EEG} and {fMRI} monitoring in humans},
  Author                   = {Portas, C M and Krakow, K and Allen, P and Josephs, O and Armony, J L and Frith, C D},
  Journal                  = {Neuron},
  Year                     = {2000},

  Month                    = dec,
  Note                     = {{PMID:} 11163282},
  Number                   = {3},
  Pages                    = {991--999},
  Volume                   = {28},

  Abstract                 = {We combined {fMRI} and {EEG} recording to study the neurophysiological responses associated with auditory stimulation across the sleep-wake cycle. We found that presentation of auditory stimuli produces bilateral activation in auditory cortex, thalamus, and caudate during both wakefulness and nonrapid eye movement ({NREM)} sleep. However, the left parietal and, bilaterally, the prefrontal and cingulate cortices and the thalamus were less activated during {NREM} sleep compared to wakefulness. These areas may play a role in the further processing of sensory information required to achieve conscious perception during wakefulness. Finally, during {NREM} sleep, the left amygdala and the left prefrontal cortex were more activated by stimuli having special affective significance than by neutral stimuli. These data suggests that the sleeping brain can process auditory stimuli and detect meaningful events.},
  ISSN                     = {0896-6273},
  Keywords                 = {Acoustic Stimulation, Adult, Amygdala, Auditory Cortex, Auditory Perception, Brain, Caudate Nucleus, Circadian Rhythm, Electroencephalography, Female, Functional Laterality, Gyrus Cinguli, Humans, Magnetic Resonance Imaging, Male, Parietal Lobe, prefrontal cortex, Sleep, Sleep Stages, Thalamus, Wakefulness},
