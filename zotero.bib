@Article{kaunitz_intercepting_2011,
  Title                    = {Intercepting the First Pass: Rapid Categorization is Suppressed for Unseen Stimuli},
  Author                   = {Kaunitz, Lisandro Nicolas and Kamienkowski, Juan Esteban and Olivetti, Emanuele and Murphy, Brian and Avesani, Paolo and Melcher, David Paul},
  Journal                  = {Frontiers in Psychology},
  Year                     = {2011},

  Month                    = aug,
  Note                     = {{PMID:} 21897827
{PMCID:} {PMC3160141}},
  Volume                   = {2},

  Abstract                 = {The operations and processes that the human brain employs to achieve fast visual categorization remain a matter of debate. A first issue concerns the timing and place of rapid visual categorization and to what extent it can be performed with an early feed-forward pass of information through the visual system. A second issue involves the categorization of stimuli that do not reach visual awareness. There is disagreement over the degree to which these stimuli activate the same early mechanisms as stimuli that are consciously perceived. We employed continuous flash suppression ({CFS)}, {EEG} recordings, and machine learning techniques to study visual categorization of seen and unseen stimuli. Our classifiers were able to predict from the {EEG} recordings the category of stimuli on seen trials but not on unseen trials. Rapid categorization of conscious images could be detected around 100 ms on the occipital electrodes, consistent with a fast, feed-forward mechanism of target detection. For the invisible stimuli, however, {CFS} eliminated all traces of early processing. Our results support the idea of a fast mechanism of categorization and suggest that this early categorization process plays an important role in later, more subtle categorizations, and perceptual processes.},
  Doi                      = {10.3389/fpsyg.2011.00198},
  File                     = {PubMed Central Full Text PDF:/Users/lcnllab/Library/Application Support/Zotero/Profiles/oh6jsc30.default/zotero/storage/4ZPIKQWB/Kaunitz et al. - 2011 - Intercepting the First Pass Rapid Categorization .pdf:application/pdf},
  ISSN                     = {1664-1078},
  Shorttitle               = {Intercepting the First Pass},
  Url                      = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3160141/},
  Urldate                  = {2013-03-11}
}

@Article{kay_color_2005,
  Title                    = {Color Categories Are Not Arbitrary},
  Author                   = {Kay, Paul},
  Journal                  = {Cross-Cultural Research},
  Year                     = {2005},

  Month                    = feb,
  Number                   = {1},
  Pages                    = {39--55},
  Volume                   = {39},

  Abstract                 = {Recent, well-controlled studies in cross-language color naming and cross-language tests of color memory and learning have made important contributions to our understanding of which aspects of cross-language color naming and nonverbal response to colors may and may not be attributed to pan-human properties of color appearance. Valuable as these results are, some studies have led to more relativistic conclusions than their results justify. In particular, these conclusions ignore the issue of whether there exists across languages a statistical tendency toward basing color terminology systems on black, white, and the four Hering opponent hues.},
  Doi                      = {10.1177/1069397104267889},
  File                     = {Snapshot:/Users/lcnllab/Library/Application Support/Zotero/Profiles/oh6jsc30.default/zotero/storage/HSMJ7AV4/39.html:text/html},
  ISSN                     = {1069-3971, 1552-3578},
