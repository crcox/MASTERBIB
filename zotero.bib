  Url                      = {http://www.sciencedirect.com/science/article/pii/S0896627304005409},
  Urldate                  = {2012-05-16}
}

@Article{walsh_effects_1992,
  Title                    = {The effects of lesions of area V4 on the visual abilities of macaques: colour categorization},
