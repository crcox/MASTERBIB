  Keywords                 = {Connectionist models, Inflectional Morphology, Langauge Processing, Parallel-Distributed Processing, past tense, Rules},
  Shorttitle               = {Rules or connections in past-tense inflections},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S1364661302019939},
  Urldate                  = {2014-03-17}
}

