  Keywords                 = {Berinmo, Color, Color cognition, color naming, Relativism, Universalism},
  Shorttitle               = {Color naming universals},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0010027705002441},
  Urldate                  = {2012-10-05}
}

