  ISSN                     = {1939-1315(Electronic);0022-3514(Print)},
  Keywords                 = {college students, humor \& affective responses, inhibition vs facilitation of smiles},
  Shorttitle               = {Inhibiting and facilitating conditions of the human smile}
}

@Article{sudre_tracking_2012,
  Title                    = {Tracking neural coding of perceptual and semantic features of concrete nouns},
