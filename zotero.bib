@Article{roberson_categorical_2008,
  Title                    = {Categorical perception of colour in the left and right visual field is verbally mediated: Evidence from Korean},
  Author                   = {Roberson, Debi and Pak, Hyensou and Hanley, J. Richard},
  Journal                  = {Cognition},
